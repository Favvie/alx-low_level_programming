#include <stdio.h>

/**
 * main - Etnry point
 *
 * Description - Print lowercase and uppercase letters
 *
 * Return: 0
 */
int main(void)
{
	char c;
	char d;
	int i;

	c = 'a';
	d = 'A';
	i = 0;

	while (c <= 'z')
	{
		putchar(c);
		c++;
	}
	while (d <= 'Z')
	{
		putchar(d);
		d++;
	}
	putchar('\n');
	return (0);
}
